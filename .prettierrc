{
  "tabWidth": 2,
  "printWidth": 110,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleQuote": false,
  "arrowParens": "always",
  "trailingComma": "none",
  "overrides": [
    {
      "files": ["*.ts", "*.css", "*.scss"],
      "options": {
        "tabWidth": 2,
        "semi": true
      }
    }
  ]
}
